<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FinanceWpf.View"
                    xmlns:model="clr-namespace:FinanceWpf.Model;assembly=FinanceWpf.Model">



    <DataTemplate DataType="{x:Type model:Trade}">


        <UniformGrid DockPanel.Dock="Top" Height="60" Rows="2" Columns="4">

            <Label Content="Date"></Label>
            <Label Content="Price"></Label>
            <Label Content="Amount"></Label>
            <Label Content="Profit"></Label>

            <TextBox Text="{Binding Date}"></TextBox>
            <TextBox Text="{Binding Price}"></TextBox>
            <TextBox Text="{Binding Amount}"></TextBox>
            <TextBox Text="{Binding Profit}"></TextBox>
        </UniformGrid>

    </DataTemplate>





</ResourceDictionary>