<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FinanceWpf.View"
                    xmlns:prdsys="clr-namespace:FinanceWpf.Model;assembly=FinanceWpf.Model">



    <DataTemplate DataType="{x:Type prdsys:Stock}">
        <UniformGrid  Height="20" Width="400" Rows="1" Columns="4">
            <Label Content="Symbol" FontSize="10"></Label>
            <TextBox Text="{Binding Key}" FontSize="10"></TextBox>
            <Label Content="Company" FontSize="10"></Label>
            <TextBox Text="{Binding Name}" FontSize="10" Width="100"></TextBox>
        </UniformGrid>
    </DataTemplate>


</ResourceDictionary>